{
  "plugins": [
    "react-require",
    [
      "auto-import",
      {
        "declarations": [
          {
            "default": "React",
            "path": "react"
          }
        ]
      }
    ]
  ],
  "presets": [
    [
      "@babel/preset-react", {
        "runtime": "automatic"
      }
    ]
  ]
}